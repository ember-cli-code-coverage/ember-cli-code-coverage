{
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:kategengler/ember-cli-code-coverage.git"
  },
  "scripts": {
    "test": "vitest"
  },
  "devDependencies": {
    "@release-it-plugins/lerna-changelog": "^6.0.0",
    "@release-it-plugins/workspaces": "^4.0.0",
    "chai-files": "^1.4.0",
    "execa": "8.0.1",
    "fixturify-project": "~6.0.0",
    "fs-extra": "~11.1.1",
    "release-it": "^16.2.1",
    "rimraf": "^5.0.5",
    "vite": "^4.5.0",
    "vitest": "^1.0.0-beta.4"
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/workspaces": {
        "additionalManifests": {
          "dependencyUpdates": [
            "test-packages/*/package.json"
          ]
        },
        "publish": false
      },
      "@release-it-plugins/lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    },
    "npm": false
  },
  "version": "2.0.3",
  "volta": {
    "node": "16.20.2",
    "pnpm": "8.10.2"
  }
}
