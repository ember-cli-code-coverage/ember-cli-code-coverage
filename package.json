{
  "private": true,
  "version": "3.0.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:kategengler/ember-cli-code-coverage.git"
  },
  "scripts": {
    "test": "vitest"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@release-it-plugins/lerna-changelog": "^6.1.0",
    "@release-it-plugins/workspaces": "^4.2.0",
    "chai-files": "^1.4.0",
    "content-tag": "^2.0.2",
    "execa": "8.0.1",
    "fixturify-project": "~6.0.0",
    "fs-extra": "~11.1.1",
    "release-it": "^16.3.0",
    "rimraf": "^5.0.10",
    "vite": "^4.5.5",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">= 18",
    "pnpm": "^9.11.0"
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/workspaces": {
        "workspaces": [
          "packages/ember-cli-code-coverage"
        ]
      },
      "@release-it-plugins/lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    },
    "npm": false
  }
}
